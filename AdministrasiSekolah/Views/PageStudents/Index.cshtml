
@model IEnumerable<AdministrasiSekolah.Models.Student>

@{
    ViewData["Title"] = "Index";
    Layout = null;
}

<h2 class="text-center" style="font-size:64px">Data Siswa</h2>
<br />
<br />
<br />
<table class="table table-striped" border="1">
    <thead class="thead-dark">
        <tr>
            <th>
                @Html.DisplayName("NIS")
            </th>
            <th>
                @Html.DisplayName("Nama Student")
            </th>
            <th>
                @Html.DisplayName("Kelas")
            </th>
            <th>
                @Html.DisplayName("Angkatan")
            </th>
            <th>
                @Html.DisplayName("Gender")
            </th>
            <th>
                @Html.DisplayName("Alamat")
            </th>
            <th>
                @Html.DisplayName("Password")
            </th>
            <th>
                @Html.DisplayName("Nama Ayah")
            </th>
            <th>
                @Html.DisplayName("Username")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nis)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NamaStudent)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Kelas)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Angkatan)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Gender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Alamat)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Password)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdParentNavigation.NamaAyah)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdUserNavigation.Username)
                </td>
            </tr>
        }
    </tbody>
</table>

